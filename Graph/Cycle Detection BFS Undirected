def cycleDetection(adj):
  vis = set()
  n = len(adj)
  for i in range(n):
    if i in vis:
      continue
    vis.add(i)
    q = deque([i,-1])
    while q:
      el, par = q.pop()
      for adjel in range(n):
        if adj[el][adjel]:
          if adjel not in vis:
            vis.add(adjel)
            q.appendleft([adjel,el])
          elif adjel != par:
            print('cycle fould at',el,'-',adjel)
            return
    print('no cycle')
          
